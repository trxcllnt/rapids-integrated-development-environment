#! /usr/bin/env bash

set -Eeo pipefail

if [ -n $(which configure-rmm-cpp) ] then configure-rmm-cpp; fi;
if [ -n $(which configure-cudf-cpp) ] then configure-cudf-cpp; fi;
if [ -n $(which configure-raft-cpp) ] then configure-raft-cpp; fi;
if [ -n $(which configure-cumlprims_mg-cpp) ] then configure-cumlprims_mg-cpp; fi;
if [ -n $(which configure-cuml-cpp) ] then configure-cuml-cpp; fi;
if [ -n $(which configure-cugraph-ops-cpp) ] then configure-cugraph-ops-cpp; fi;
if [ -n $(which configure-cugraph-cpp) ] then configure-cugraph-cpp; fi;
if [ -n $(which configure-cuspatial-cpp) ] then configure-cuspatial-cpp; fi;
